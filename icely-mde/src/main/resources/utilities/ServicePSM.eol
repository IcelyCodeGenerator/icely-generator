// SERVICEPSM

operation ServicePSM!Relationship setMultiplicity(multiplicity){
	if(multiplicity == ServicePSM!Multiplicity#ONE_TO_MANY.name){
  		self.multiplicity = ServicePSM!Multiplicity#ONE_TO_MANY;
  	} else if(multiplicity == ServicePSM!Multiplicity#MANY_TO_ONE.name){
  		self.multiplicity = ServicePSM!Multiplicity#MANY_TO_ONE;
  	} else if(multiplicity == ServicePSM!Multiplicity#MANY_TO_MANY.name){
  		self.multiplicity = ServicePSM!Multiplicity#MANY_TO_MANY;
  	} else {
  		self.multiplicity = ServicePSM!Multiplicity#ONE_TO_ONE;
  	}
}

operation ServicePSM!Project getApplications(){
	return self.applications;
}

operation ServicePSM!Project getEntities(){
	return self.entities;
}

operation ServicePSM!Application getEntities(){
	return self.entities;
}

operation ServicePSM!Project findEntity(name:String){
	for (entity in self.entities){
		if (entity.name == name){
			return entity;
		}
	}
}

operation ServicePSM!Project findEntity(name:String, applicationName: String){
	var application = self.findApplication(applicationName);
	return application.findEntity(name);
}

operation ServicePSM!Application findEntity(name:String){
	for (entity in self.entities){
		if (entity.name == name){
			return entity;
		}
	}
	return null;
}

operation OrderedSet findCommonApplication(firstApplications, secondApplications){
	var applications = new OrderedSet<String>();
	for (firstApp in firstApplications){
		for(secondApp in secondApplications){
			if (firstApp.hasConfig.baseName == secondApp.hasConfig.baseName){
				applications.add(firstApp.hasConfig.baseName);
			} 
		}
	}
	return applications;
}

operation ServicePSM!Project findApplicationIndex(name:String){
	var i = 0;
	for (application in self.applications){
		if (application.name == name){
			return i;
		}
		i++;
	}
	return -1;
}

operation ServicePSM!Project findApplication(name:String){
	for (application in self.applications){
		if (application.name == name){
			return application;
		}
	}
}

operation ServicePSM!Relation relationshipInstance(){
	if(self.multiplicity.name == ServicePSM!Multiplicity#MANY_TO_MANY.name or self.multiplicity.name == ServicePSM!Multiplicity#ONE_TO_ONE.name){
		return new ServicePSM!BidirectionalRelationship;
	} else if(self.multiplicity.name == ServicePSM!Multiplicity#ONE_TO_MANY.name or self.multiplicity.name == ServicePSM!Multiplicity#MANY_TO_ONE.name){
		return new ServicePSM!UnidirectionalRelationship;
	}
}

operation ServicePSM!Module findResource(name:String){
	for (resource in self.hasResources){
		if (resource.name == name){
			return resource;
		}
	}
}